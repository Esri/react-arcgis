{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": true,
        "declaration": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "suppressImplicitAnyIndexErrors": true
    },
    "include": [
        "./index.ts",
        "./src/**/*"
    ]
}